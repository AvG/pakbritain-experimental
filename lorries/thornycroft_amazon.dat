# 1930s articulated vehicle. Although a large number of different types of "Amazon"s were produced,
# this is intended to depict one of the later versions.
# See the picture of the articulated tractor here: http://www3.hants.gov.uk/museum/thornycroft/lorries-2/model-range-1933/amazon.htm
# See here (look towards the bottom) for more pictures of Amazons: http://www.thornycroft.org.uk/Thornycroft%20Group%20B.htm

obj=vehicle
name=thornycroft_amazon_tractor
copyright=jamespettsandgraphic_author
waytype=road
engine_type=diesel
smoke=Diesel
power=61
speed=50
weight=5
cost=137000
runningcost=150
intro_year=1933
intro_month=2
retire_year=1949
retire_month=9

constraint[next][0]=none 
constraint[prev][0]=thornycroft_amazon_trailer_goods
constraint[prev][1]=thornycroft_amazon_trailer_bulk
constraint[prev][2]=thornycroft_amazon_trailer_food
constraint[prev][3]=thornycroft_amazon_trailer_long
constraint[prev][4]=thornycroft_amazon_trailer_oil

# The below will need changing

emptyimage[w]=generic_road_vehicle.0.1
emptyimage[nw]=generic_road_vehicle.0.2
emptyimage[n]=generic_road_vehicle.0.3
emptyimage[ne]=generic_road_vehicle.0.4
emptyimage[e]=generic_road_vehicle.0.5
emptyimage[se]=generic_road_vehicle.0.6
emptyimage[s]=generic_road_vehicle.0.7
emptyimage[sw]=generic_road_vehicle.0.8

#Simutrans-Experimental specific settings: ignored by standard Makeobj

loading_time=0
#upgrade[0]=
#upgrade_price=
available_only_as_upgrade=0
fixed_maintenance=0
overcrowded_capacity=0
comfort=0

